    "version": "0.2.0",
    "configurations": [
        {
            "name": "Kubernetes: Run/Debug - hot-reload",
            "type": "cloudcode.kubernetes",
            "request": "launch",
            "skaffoldConfig": "${workspaceFolder}/skaffold.yaml",
            "profile": "hot-reload",
            "watch": true,
            "cleanUp": true,
            "portForward": true,
            "internalConsoleOptions": "neverOpen",
            "imageRegistry": "insost/jagw-subscription-service"
        }
    ]
}